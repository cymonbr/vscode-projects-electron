{
	"name": "vscode-projects",
	"version": "1.0.0",
	"description": "",
	"main": "main.js",
	"scripts": {
		"modules:init": "npm install && cd app && npm install && cd ..",
		"start": "electron .",
		"server": "node src/server.js",
		"server:watch": "nodemon src/server.js",
		"app": "cd app && npm run start",
		"app:build": "cd app && npm run build && cd ..",
		"build-win:dev": "node node_modules/electron-packager/bin/electron-packager.js . \"VSCode Projects\" --version-string.ProductName=\"VSCode Projects\" --electron-version=11.2.3 --platform=win32 --arch=x64 --icon ./src/images/icon.ico --prune=true --out=./dist --ignore=\"app/node_modules\" --ignore=\"app/public\" --ignore=\"app/src\" --ignore=\"app/config-overrides.js\" --ignore=\"app/tsconfig.json\" --ignore=\"app/tsconfig.paths.json\" --ignore=\".git(ignore|modules)\" --overwrite --no-tmpdir",
		"build-win": "node node_modules/electron-packager/bin/electron-packager.js . \"VSCode Projects\" --version-string.ProductName=\"VSCode Projects\" --electron-version=11.2.3 --platform=win32 --arch=x64 --icon ./src/images/icon.ico --asar --prune=true --out=./dist --ignore=\"app/node_modules\" --ignore=\"app/public\" --ignore=\"app/src\" --ignore=\"app/config-overrides.js\" --ignore=\"app/tsconfig.json\" --ignore=\"app/tsconfig.paths.json\" --ignore=\".git(ignore|modules)\" --overwrite --no-tmpdir"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"body-parser": "^1.19.0",
		"cheerio": "^1.0.0-rc.3",
		"electron-store": "^7.0.2",
		"express": "^4.17.1",
		"express-async-errors": "^3.1.1",
		"request": "^2.88.2",
		"request-promise": "^4.2.6",
		"validator": "^13.1.17"
	},
	"devDependencies": {
		"electron": "^11.2.3",
		"electron-packager": "^15.2.0"
	}
}
